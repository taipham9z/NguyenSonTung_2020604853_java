package exercise7;

import java.util.Scanner;

public class ResizableCircle extends Circle implements IReszable  {
	Scanner scanner = new Scanner(System.in);
	public ResizableCircle(double radius) {
		this.radius = radius;
	}

	@Override
	public void resize(int percent) {
		this.radius += percent;
	}
	
	
	
	
	
	
}
